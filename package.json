{
  "name": "video-shortcut-extractor",
  "version": "1.0.0",
  "description": "跨平台视频快捷方式提取工具",
  "main": "src/main/main.js",
  "scripts": {
    "start": "npm run build:dev && electron .",
    "start:dev": "webpack serve --config config/webpack.config.js --mode development",
    "build:dev": "webpack --config config/webpack.config.js --mode development",
    "build:prod": "webpack --config config/webpack.config.js --mode production",
    "build": "npm run build:prod && electron-builder",
    "build:mac": "npm run build:prod && electron-builder --mac",
    "build:win": "npm run build:prod && electron-builder --win",
    "build:linux": "npm run build:prod && electron-builder --linux"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.23.7",
    "@babel/preset-env": "^7.23.7",
    "asar": "^3.2.0",
    "babel-loader": "^9.1.3",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^6.8.1",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "electron": "^28.3.3",
    "electron-builder": "^24.9.1",
    "html-webpack-plugin": "^5.6.0",
    "mini-css-extract-plugin": "^2.7.6",
    "style-loader": "^3.3.4",
    "terser-webpack-plugin": "^5.3.10",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "build": {
    "appId": "com.videoextractor.app",
    "productName": "wcs工具箱",
    "directories": {
      "output": "release"
    },
    "npmRebuild": false,
    "files": [
      "dist/**/*",
      "src/main/main.js",
      "src/main/preload.js",
      "node_modules/**/*",
      "!node_modules/**/test/**",
      "!node_modules/**/*.md"
    ],
    "extraResources": [],
    "asarUnpack": [
      "node_modules/7zip-bin/**/*"
    ],
    "mac": {
      "category": "public.app-category.utilities",
      "target": [
        "dmg",
        "zip"
      ]
    },
    "win": {
      "target": [
        "portable"
      ],
      "signAndEditExecutable": false
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    }
  },
  "dependencies": {
    "7zip-bin": "^5.2.0",
    "7zip-min": "^2.1.0",
    "archiver": "^7.0.1",
    "cheerio": "^1.0.0-rc.12",
    "epub-gen-memory": "^1.1.2",
    "iconv-lite": "^0.7.2",
    "jszip": "^3.10.1",
    "minimatch": "^9.0.3",
    "readdir-glob": "^1.1.3"
  }
}